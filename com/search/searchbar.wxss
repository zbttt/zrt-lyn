/* com/search/searchbar.wxss */
.searchbar{
    background-color: #41be57;
    padding: 20rpx;
}

.search{
  width: 100%;
  height: 60rpx;
  border-radius: 20rpx;
  background-color: #41be57;
  background-image: url("data:image/png;base64,");
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 8%;
}

.search-input{
  width: 100%;
  height: 60rpx;
  border-radius: 20rpx;
  background-color: #eaf5ec;
  padding: 5rpx 20rpx;
  box-sizing: border-box;
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 8%;
  background-image: url("data:image/png;base64,");
}

.input{
  min-height: 40rpx;
  height: 40rpx;
  font-size: 12px;
}